create table Type 
(
	ID		int(3),
	TypeName	varchar(60),
constraint book1_pk primary key (ID)
);

create table Item 
(
	ID		int(3),
    TypeID 	int(3),
    ItemID		CHAR(11),
	ItemName		CHAR(11),
	Detail		CHAR(100),
    Price 	int(10),
    Building		CHAR(11),
constraint Item_pk primary key (ID),
constraint Item_fk foreign key (TypeID)
	references Type(TypeID)
);